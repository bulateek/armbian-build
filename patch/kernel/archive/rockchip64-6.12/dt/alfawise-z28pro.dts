	gpio_led: gpio-leds {
		compatible = "gpio-leds";

		working {
			gpios = <&rk805 0 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "heartbeat";
			//default-state = "off";
			//mode = <35>;
		};

		auxiliary {
			gpios = <&rk805 1 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "mmc2";
			//default-state = "off";
			//mode = <5>;
		};
